{
  "name": "nativescript-drop-down",
  "version": "5.0.6",
  "description": "A NativeScript DropDown widget.",
  "main": "drop-down",
  "typings": "drop-down.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/NativeScript-Drop-Down"
  },
  "scripts": {
    "setup": "npm i && ts-patch install",
    "tslint": "tslint --project tsconfig.json",
    "tsc": "tsc --project tsconfig.json",
    "ngc": "ngc --project tsconfig.aot.json",
    "demo-ios": "grunt build && cd demo && tns run ios",
    "demo-android": "grunt build && cd demo && tns run android",
    "demo-ng-ios": "grunt build && cd demo-ng && tns run ios",
    "demo-ng-android": "grunt build && cd demo-ng && tns run android"
  },
  "keywords": [
    "NativeScript",
    "DropDown",
    "Spinner",
    "tangra"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "contributors": [
    "Dick Smith (https://github.com/DickSmith)",
    "AWhiteSheep (https://github.com/AWhiteSheep)"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "tns-core-modules": ">= 5"
  },
  "devDependencies": {
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/router": "^5.2.11",
    "@nativescript/core": "^7.0.0",
    "@nativescript/types": "^7.0.0",
    "@nativescript/webpack": "^3.0.0",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-exec": "2.0.0",
    "nativescript-angular": "^5.3.0",
    "rxjs": "~5.5.6",
    "ts-patch": "^1.3.0",
    "tslint": "^4.5.1",
    "typescript": "^3.9.7",
    "zone.js": "~0.8.12"
  },
  "dependencies": {
    "ts-node": "^9.0.0"
  }
}
